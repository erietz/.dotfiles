{
  "shebang": {
    "prefix": "shebang",
    "body":[
        "#!/usr/bin/env zsh",
        "$0"
    ],    
    "description": "#!/usr/bin/env zsh"
  },
  "gaussian-slurm": {
    "prefix": "gaussian-slurm",
    "body":[
        "#!/usr/bin/env zsh",
        "#SBATCH --nodes=1",
        "#SBATCH --ntasks-per-node=16",
        "#SBATCH --mem=10gb",
        "#SBATCH --job-name=${1:jobname}",
        "${2:#SBATCH --mail-user=ewrietz@iu.edu}",
        "${3:#SBATCH --mail-type=FAIL,BEGIN,END}",
        "#SBATCH --time=${5:0-00:20:00}",
        "",
        "cd $SLURM_SUBMIT_DIR",
        "g16 < ${4:filename}.com > $4.log",
        "",
        "$0",
        ""
    ],    
    "description": "slurm gaussian job"
  },
  "slurm": {
    "prefix": "slurm",
    "body":[
        "#!/usr/bin/env zsh",
        "#SBATCH --nodes=1",
        "#SBATCH --ntasks-per-node=48",
        "#SBATCH --mem=40gb",
        "#SBATCH --job-name=${1:jobname}",
        "${2:#SBATCH --mail-user=ewrietz@iu.edu}",
        "${3:#SBATCH --mail-type=FAIL,BEGIN,END}",
        "#SBATCH --time=${4:0-00:20:00}",
        "",
        "$0"
    ],    
    "description": "general slurm job"
  },
  "pbs": {
    "prefix": "pbs",
    "body":[
        "#!/usr/bin/env zsh",
        "#PBS -l nodes=1:ppn=16",
        "#PBS -l vmem=10gb",
        "#PBS -l walltime=${1:0-00:20:00}",
        "#PBS -N ${2:jobname}",
        "${3:#PBS -M ewrietz@iu.edu}",
        "${4:#PBS -m abe}",
        "",
        "g16 < ${5:filename}.com > $5.log",
        "",
        "$0"
    ],    
    "description": "pbs gaussian job"
  }
}
