    "body": "#!/usr/bin/env zsh\n#SBATCH --nodes=1\n#SBATCH --ntasks-per-node=16\n#SBATCH --mem=10gb\n#SBATCH --time=${1:0-00:20:00}\n#SBATCH --job-name=${2:jobname}\n${3:#SBATCH --mail-user=ewrietz@iu.edu}\n${4:#SBATCH --mail-type=FAIL,BEGIN,END}\n\ncd $SLURM_SUBMIT_DIR\ng16 < ${5:filename}.com > $5.log\n\n$0\n",
