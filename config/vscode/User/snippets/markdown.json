{
    "/": {
        "prefix": "/",
        "description": "Fraction",
        "body": [
            "\\frac{${1:${TM_SELECTED_TEXT:numerator}}}{${2:denominator}}${0}"
        ]
    },
    "E": {
        "prefix": "E",
        "description": "Power of 10",
        "body": [
            "\\times 10^{$1}$0"
        ]
    },
    "sum": {
        "prefix": "sum",
        "description": "Summation",
        "body": [
            "\\sum\\limits_{${1:n}}^{${2:\\infty}}$0"
        ]
    },
    "*": {
        "prefix": "*",
        "description": "times",
        "body": [
            "\\times $0"
        ]
    },
    "sympy": {
        "prefix": "sympy",
        "description": "sympy block",
        "body": [
            "sympy $1 sympy$0"
        ]
    },
    "'sympy(.*)sympy'": {
        "prefix": "'sympy(.*)sympy'",
        "description": "evaluate sympy",
        "body": [
            "`!p",
            "from sympy import *",
            "x, y, z, t = symbols('x y z t')",
            "k, m, n = symbols('k m n', integer=True)",
            "f, g, h = symbols('f g h', cls=Function)",
            "init_printing()",
            "snip.rv = eval('latex(' + match.group(1).replace('\\\\', '') \\",
            ".replace('^', '**') \\",
            ".replace('{', '(') \\",
            ".replace('}', ')') + ')')",
            "`"
        ]
    },
    "link": {
        "prefix": "link",
        "description": "Link to something",
        "body": [
            "[${1:${TM_SELECTED_TEXT:Text}}](${3:http://${2:www.url.com}})$0"
        ]
    },
    "img": {
        "prefix": "img",
        "description": "Image",
        "body": [
            "![${1:pic alt}](${2:path}${3/.+/ \"/}${3:opt title}${3/.+/\"/})$0"
        ]
    },
    "ilc": {
        "prefix": "ilc",
        "description": "Inline Code",
        "body": [
            "\\`$1\\`$0"
        ]
    },
    "cbl": {
        "prefix": "cbl",
        "description": "Codeblock",
        "body": [
            "\\`\\`\\`",
            "$1",
            "\\`\\`\\`",
            "$0"
        ]
    },
    "refl": {
        "prefix": "refl",
        "description": "Reference Link",
        "body": [
            "[${1:${TM_SELECTED_TEXT:Text}}][${2:id}]$0",
            "",
            "[$2]:${4:http://${3:www.url.com}} \"${5:$4}\""
        ]
    },
    "fnt": {
        "prefix": "fnt",
        "description": "Footnote",
        "body": [
            "[^${1:${TM_SELECTED_TEXT:Footnote}}]$0",
            "",
            "[^$1]:${2:Text}"
        ]
    },
    "todo": {
        "prefix": "todo",
        "description": "Todo checkbox",
        "body": [
            "- [${1:space or an \"x\"}] ${0:item that needs done}"
        ]
    }
}