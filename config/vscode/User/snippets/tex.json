{
  "preamble": {
    "prefix": "preamble",
    "body": [
    "\\documentclass{article}",
    "\\usepackage{geometry}",
    "\\usepackage{hyperref}",
    "\\hypersetup{",
        "colorlinks=true",
        "linkcolor=blue}",
    "\\usepackage{longtable,booktabs}",
    "%\\usepackage[style=chem-acs, autocite=superscript]{biblatex}",
    "%\\addbibresource{ref.bib}",
    "%\\usepackage{physics}",
    "%\\setcounter{secnumdepth}{0}",
    "%\\usepackage[usenames,dvipsnames]{xcolor}",
    "%\\usepackage{siunitx}",
    "%\\usepackage{mhchem}",
    "\\title{${1:Title}}",
    "\\author{Ethan Rietz}",
    "\\date{\\today}",
    "\\begin{document}",
    "\\maketitle",
    "$0",
    "\\end{document}"
    ],
    "description": "Preamble"
  },
  "up": {
    "prefix": "up",
    "body": "\\usepackage{${1:package}}$0",
    "description": "Use Package"
  },
  "\"begin\"": {
    "prefix": "\"beg(in)?\"",
    "body": [
        "\\begin{${1:<environment>}}",
        "\t$0",
        "\\end{$1}"
    ],
    "description": "begin{} / end{}"
  },
  "nc": {
    "prefix": "nc",
    "body": "{\\\\${1:cmd}}[${2:opt}]{${3:realcmd}} ${0}",
    "description": "New Command"
  },
  "\"fig(ure)?\"": {
    "prefix": "\"fig(ure)?\"",
    "body": "\\begin{figure}[${2:htpb}]",
    \t\\centering",
    "\t\\includegraphics[width=${3:0.8}\\linewidth]{${4:name.ext}}",
    "\t\\caption{$0}",
    \t\\label{fig:${4}}",
    \\end{figure}",
    "description": "Figure environment"
  },
  "\"tab(le)?\"": {
    "prefix": "\"tab(le)?\"",
    "body": "\\begin{table}[${1:htpb}]",
    \t\\centering",
    "\t\\caption{${2:caption}}",
    "\t\\label{tab:${3:label}}",
    \t\\begin{tabular}{${4:c}}",
    \t$0",
    \t\\end{tabular}",
    "\\end{table}",
    "description": "Table environment"
  },
  "\"tabu(lar)?\"": {
    "prefix": "\"tabu(lar)?\"",
    "body": "\\begin{tabular}{${1:c}}",
    "$0",
    \\end{tabular}",
    "description": "Tabular environment"
  },
  "\"sec(tion)?\"": {
    "prefix": "\"sec(tion)?\"",
    "body": "\\section{${1:${VISUAL:section name}}}",
    $,
    0",
    "description": "Section"
  },
  "\"sub(section)?\"": {
    "prefix": "\"sub(section)?\"",
    "body": "\\subsection{${1:${VISUAL:Subsection name}}}",
    "",
    $0",
    "description": "Subsection"
  },
  "\"subsub(section)?\"": {
    "prefix": "\"subsub(section)?\"",
    "body": "\\subsubsection{${1:${VISUAL:Subsubsection name}}}",
    "",
    $0",
    "description": "Subsubsection"
  },
  "\"lab(el)?\"": {
    "prefix": "\"lab(el)?\"",
    "body": "\\label{${1:${VISUAL:label name}}} $0",
    "description": "Section"
  },
  "\"i(tem)?\"": {
    "prefix": "\"i(tem)?\"",
    "body": "\\item{${1:${VISUAL:The item}}}",
    $0",
    "description": "Item"
  },
  "\"d(tem)?\"": {
    "prefix": "\"d(tem)?\"",
    "body": "\\item[${1:Description word}]{${2:${VISUAL:The item}}}",
    $0",
    "description": "Description item"
  },
  "it": {
    "prefix": "it",
    "body": "\\textit{${1:${VISUAL:italics}}} $0",
    "description": "Italics"
  },
  "bf": {
    "prefix": "bf",
    "body": "\\textbf{${1:${VISUAL:italics}}} $0",
    "description": "Boldfont"
  },
  "si": {
    "prefix": "si",
    "body": "\\SI{${1:Number}}{${2:Units}} ${0}",
    "description": "SI Units"
  },
  "question": {
    "prefix": "question",
    "body": "\\question[${1:Points}] ${2:The questions}",
    "\\begin{solution}",
    "${0}",
    "\\end{solution}",
    "\\vspace{\\stretch{1}}",
    "description": "Question"
  },
  "choices": {
    "prefix": "choices",
    "body": "\\choice{${1:${VISUAL:The choice}}}",
    "description": "Choices"
  },
  "ce": {
    "prefix": "ce",
    "body": "\\ce{$1} $0",
    "description": "Chemical equation"
  }
}
