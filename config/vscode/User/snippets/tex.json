{
    "preamble": {
        "prefix": "preamble",
        "description": "Preamble",
        "body": [
            "\\documentclass{article}",
            "\\usepackage{geometry}",
            "\\usepackage{hyperref}",
            "\\hypersetup{",
            "colorlinks=true,",
            "linkcolor=blue}",
            "\\usepackage{longtable,booktabs}",
            "%\\usepackage[style=chem-acs, autocite=superscript]{biblatex}",
            "%\\addbibresource{ref.bib}",
            "%\\usepackage{physics}",
            "%\\setcounter{secnumdepth}{0}",
            "%\\usepackage[usenames,dvipsnames]{xcolor}",
            "%\\usepackage{siunitx}",
            "%\\usepackage{mhchem}",
            "",
            "\\title{${1:Title}}",
            "\\author{Ethan Rietz}",
            "\\date{\\today}",
            "",
            "\\begin{document}",
            "\\maketitle",
            "",
            "$0",
            "",
            "\\end{document}"
        ]
    },
    "up": {
        "prefix": "up",
        "description": "Use Package",
        "body": [
            "\\usepackage{${1:package}}$0"
        ]
    },
    "\"beg(in)?\"": {
        "prefix": "\"beg(in)?\"",
        "description": "beg(in)?",
        "body": [
            "\\begin{${1:<environment>}}",
            "${0:${TM_SELECTED_TEXT}}",
            "\\end{$1}"
        ]
    },
    "nc": {
        "prefix": "nc",
        "description": "New Command",
        "body": [
            "\\\\newcommand{\\\\${1:cmd}}[${2:opt}]{${3:realcmd}} ${0}"
        ]
    },
    "\"fig(ure)?\"": {
        "prefix": "\"fig(ure)?\"",
        "description": "fig(ure)?",
        "body": [
            "\\begin{figure}[${2:htpb}]",
            "\\centering",
            "\\includegraphics[width=${3:0.8}\\linewidth]{${4:name.ext}}",
            "\\caption{$0}",
            "\\label{fig:${4}}",
            "\\end{figure}"
        ]
    },
    "\"tab(le)?\"": {
        "prefix": "\"tab(le)?\"",
        "description": "tab(le)?",
        "body": [
            "\\begin{table}[${1:htpb}]",
            "\\centering",
            "\\caption{${2:caption}}",
            "\\label{tab:${3:label}}",
            "\\begin{tabular}{${4:c}}",
            "$0",
            "\\end{tabular}",
            "\\end{table}"
        ]
    },
    "\"tabu(lar)?\"": {
        "prefix": "\"tabu(lar)?\"",
        "description": "tabu(lar)?",
        "body": [
            "\\begin{tabular}{${1:c}}",
            "$0",
            "\\end{tabular}"
        ]
    },
    "\"sec(tion)?\"": {
        "prefix": "\"sec(tion)?\"",
        "description": "sec(tion)?",
        "body": [
            "\\section{${1:${TM_SELECTED_TEXT:section name}}}",
            "",
            "$0"
        ]
    },
    "\"sub(section)?\"": {
        "prefix": "\"sub(section)?\"",
        "description": "sub(section)?",
        "body": [
            "\\subsection{${1:${TM_SELECTED_TEXT:Subsection name}}}",
            "",
            "$0"
        ]
    },
    "\"subsub(section)?\"": {
        "prefix": "\"subsub(section)?\"",
        "description": "subsub(section)?",
        "body": [
            "\\subsubsection{${1:${TM_SELECTED_TEXT:Subsubsection name}}}",
            "",
            "$0"
        ]
    },
    "\"lab(el)?\"": {
        "prefix": "\"lab(el)?\"",
        "description": "lab(el)?",
        "body": [
            "\\label{${1:${TM_SELECTED_TEXT:label name}}} $0"
        ]
    },
    "\"i(tem)?\"": {
        "prefix": "\"i(tem)?\"",
        "description": "i(tem)?",
        "body": [
            "\\item{${1:${TM_SELECTED_TEXT:The item}}}",
            "$0"
        ]
    },
    "\"d(tem)?\"": {
        "prefix": "\"d(tem)?\"",
        "description": "d(tem)?",
        "body": [
            "\\item[${1:Description word}]{${2:${TM_SELECTED_TEXT:The item}}}",
            "$0"
        ]
    },
    "it": {
        "prefix": "it",
        "description": "Italics",
        "body": [
            "\\textit{${1:${TM_SELECTED_TEXT:italics}}} $0"
        ]
    },
    "bf": {
        "prefix": "bf",
        "description": "Boldfont",
        "body": [
            "\\textbf{${1:${TM_SELECTED_TEXT:italics}}} $0"
        ]
    },
    "si": {
        "prefix": "si",
        "description": "SI Units",
        "body": [
            "\\SI{${1:Number}}{${2:Units}} ${0}"
        ]
    },
    "\"que(stion)?\"": {
        "prefix": "\"que(stion)?\"",
        "description": "que(stion)?",
        "body": [
            "\\question[${1:Points}] ${2:The questions}",
            "",
            "\\begin{solution}",
            "${0}",
            "\\end{solution}",
            "\\vspace{\\stretch{1}}"
        ]
    },
    "\"c(hoices)?\"": {
        "prefix": "\"c(hoices)?\"",
        "description": "c(hoices)?",
        "body": [
            "\\choice{${1:${TM_SELECTED_TEXT:The choice}}}"
        ]
    },
    "ce": {
        "prefix": "ce",
        "description": "Chemical equation",
        "body": [
            "\\ce{$1} $0"
        ]
    },
    "/": {
        "prefix": "/",
        "description": "Fraction",
        "body": [
            "\\frac{${1:${TM_SELECTED_TEXT:numerator}}}{${2:denominator}}${0}"
        ]
    },
    "E": {
        "prefix": "E",
        "description": "Power of 10",
        "body": [
            "\\times 10^{$1}$0"
        ]
    },
    "sum": {
        "prefix": "sum",
        "description": "Summation",
        "body": [
            "\\sum\\limits_{${1:n}}^{${2:\\infty}}$0"
        ]
    },
    "*": {
        "prefix": "*",
        "description": "times",
        "body": [
            "\\times $0"
        ]
    },
    "sympy": {
        "prefix": "sympy",
        "description": "sympy block",
        "body": [
            "sympy $1 sympy$0"
        ]
    },
    "'sympy(.*)sympy'": {
        "prefix": "'sympy(.*)sympy'",
        "description": "evaluate sympy",
        "body": [
            "`!p",
            "from sympy import *",
            "x, y, z, t = symbols('x y z t')",
            "k, m, n = symbols('k m n', integer=True)",
            "f, g, h = symbols('f g h', cls=Function)",
            "init_printing()",
            "snip.rv = eval('latex(' + match.group(1).replace('\\\\', '') \\",
            ".replace('^', '**') \\",
            ".replace('{', '(') \\",
            ".replace('}', ')') + ')')",
            "`"
        ]
    }
}